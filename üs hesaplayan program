import java.util.*;
import java.io.*;

class Solution{
    public static void main(String []argh) {
        int taban,us,sonuc=1;

        Scanner input = new Scanner(System.in);
        System.out.println("Tabanı Giriniz: ");
        taban=input.nextInt();
        System.out.println("Üssü Giriniz: ");
        us=input.nextInt();
        for(int i=0; i<us; i++)
        {
            sonuc*=taban;
        }
        System.out.println("sonuç: "+ sonuc);


    }

}

